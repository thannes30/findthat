<div class="container">
  <h3>Welcome <%= current_user.email %></h3>
    <div class="row">

      <div class="span8">
        <h4>Check Ins</h4>
          <ul class="checkins-ul">
            <% @checkins.each do |checkin| %>
            <li class="checkins-li">
              <%= checkin.name %>
            </li>
            <% end %>
          </ul>
      </div>

      <div class="span4">
        <h4>Likes</h4>
          <ul class="likes-ul">

          </ul>
      </div>

    </div>
</div>
